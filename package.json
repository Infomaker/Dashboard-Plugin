{
  "name": "dashboard-plugin",
  "version": "1.8.0",
  "license": "ISC",
  "description": "Infomaker Dashboard Starter Plugin",
  "homepage": "https://github.com/Infomaker/Dashboard-Plugin",
  "repository": "https://github.com/Infomaker/Dashboard-Plugin",
  "private": true,
  "bugs": "https://github.com/Infomaker/Dashboard-Plugin/issues",
  "main": "__tooling__/webpack/webpack.dev.config.js",
  "scripts": {
    "start": "npm-run-all prompt --parallel server dev",
    "start:hot": "npm-run-all prompt --parallel server:hot dev:hot",
    "prompt": "node ./prompt.js",
    "dev": "webpack --config __tooling__/webpack/webpack.dev.config.js --mode development",
    "dev:hot": "webpack --config __tooling__/webpack/webpack.dev.hot.config.js --mode development",
    "server": "node server.js ",
    "server:hot": "HOT=1 node server.js ",
    "upload:s3": "yarn build:prod && node ./s3.js",
    "build:prod": "webpack --config __tooling__/webpack/webpack.prod.config.js --mode production -p"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^0.1.1",
    "autoprefixer": "^6.4.0",
    "aws-sdk": "^2.4.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "cors": "^2.8.1",
    "css-loader": "^1.0.0",
    "eslint": "^3.5.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-react": "^6.2.2",
    "express": "^4.14.0",
    "ip": "^1.1.5",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "^1.11.0",
    "npm": "^4.0.3",
    "npm-run-all": "^3.1.2",
    "os": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "prompt": "1.0.0",
    "react-hot-loader": "^4.3.12",
    "s3": "^4.4.0",
    "string-replace-loader": "^1.0.3",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.6",
    "url-loader": "^0.5.7",
    "webpack": "4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4"
  }
}
